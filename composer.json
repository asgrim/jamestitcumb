{
    "name": "asgrim/jamestitcumb",
    "description": "My personal website at www.jamestitcumb.com",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-zend-opcache": "*",
        "elasticsearch/elasticsearch": "^7.3",
        "michelf/php-markdown": "^1.8",
        "monolog/monolog": "^2.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1",
        "symfony/console": "^4.3",
        "symfony/yaml": "^4.3",
        "webmozart/assert": "^1.5",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-expressive": "^3.2",
        "zendframework/zend-expressive-fastroute": "^3.0",
        "zendframework/zend-expressive-helpers": "^5.3",
        "zendframework/zend-expressive-router": "^3.1",
        "zendframework/zend-expressive-template": "^2.0",
        "zendframework/zend-expressive-zendviewrenderer": "^2.2",
        "zendframework/zend-feed": "^2.12",
        "zendframework/zend-httphandlerrunner": "^1.1",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-stratigility": "^3.2",
        "zendframework/zend-view": "^2.11"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.4",
        "doctrine/coding-standard": "^6.0",
        "zendframework/zend-expressive-tooling": "^1.2",
        "vimeo/psalm": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Asgrim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsgrimTest\\": "test/unit/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "scripts": {
        "post-update-cmd": [
            "./app.php index-posts"
        ],
        "post-install-cmd": [
            "./app.php index-posts"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}

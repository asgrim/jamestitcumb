{
    "name": "asgrim/jamestitcumb",
    "description": "My personal website at www.jamestitcumb.com",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-intl": "*",
        "ext-pcre": "*",
        "ext-zend-opcache": "*",
        "symfony/console": "^4.3",
        "symfony/yaml": "^4.3",
        "michelf/php-markdown": "^1.8",
        "zendframework/zend-feed": "^2.12",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-expressive": "^3.2",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-expressive-fastroute": "^3.0",
        "zendframework/zend-expressive-zendviewrenderer": "^2.2",
        "zendframework/zend-config-aggregator": "^1.1",
        "elasticsearch/elasticsearch": "^7.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.4",
        "doctrine/coding-standard": "^6.0",
        "zendframework/zend-expressive-tooling": "^1.2",
        "vimeo/psalm": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Asgrim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsgrimTest\\": "test/unit/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "scripts": {
        "post-update-cmd": [
            "./app.php index-posts"
        ],
        "post-install-cmd": [
            "./app.php index-posts"
        ]
    }
}

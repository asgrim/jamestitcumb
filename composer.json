{
    "name": "asgrim/jamestitcumb",
    "description": "My personal website at www.jamestitcumb.com",
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "michelf/php-markdown": "^1.8",
        "zendframework/zend-feed": "^2.9",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-expressive": "^3.0@rc",
        "zendframework/zend-expressive-fastroute": "^3.0@rc",
        "zendframework/zend-expressive-zendviewrenderer": "^2.0@alpha",
        "zendframework/zend-config-aggregator": "^1.1",
        "elasticsearch/elasticsearch": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "friendsofphp/php-cs-fixer": "^2.3",
        "zendframework/zend-expressive-tooling": "^1.0@alpha"
    },
    "autoload": {
        "psr-4": {
            "Asgrim\\": "src/Asgrim"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsgrimTest\\": "test/unit/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "scripts": {
        "post-update-cmd": [
            "./app.php index-posts"
        ],
        "post-install-cmd": [
            "./app.php index-posts"
        ]
    }
}
